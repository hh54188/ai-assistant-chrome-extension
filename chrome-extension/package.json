{
  "name": "chrome-extension",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && node build-extension.js && node create-icons.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:stores": "vitest --run src/tests/stores/",
    "test:stores:watch": "vitest --watch src/tests/stores/",
    "test:stores:coverage": "vitest --coverage --run src/tests/stores/ --coverage.include='src/stores/**/*Store.js'",
    "test:components": "vitest --run src/tests/components/",
    "test:components:watch": "vitest --watch src/tests/components/",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "vitest --run tests/e2e/",
    "test:e2e:watch": "vitest --watch tests/e2e/",
    "test:e2e:image": "vitest --run tests/e2e/image-upload.e2e.test.js",
    "test:e2e:chat": "vitest --run tests/e2e/gemini-chat.e2e.test.js",
    "test:e2e:drag-drop": "vitest --run tests/e2e/drag-drop.e2e.test.js"
  },
  "dependencies": {
    "@ant-design/x": "^1.6.1",
    "@google/genai": "^1.15.0",
    "antd": "^5.26.4",
    "antd-style": "^3.7.1",
    "axios": "^1.12.0",
    "clipboard": "^2.0.11",
    "copy-to-clipboard": "^3.3.3",
    "dotenv": "^17.2.2",
    "html2canvas": "^1.4.1",
    "immer": "^10.1.1",
    "markdown-it": "^14.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^3.2.4",
    "clipboardy": "^4.0.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "puppeteer": "^24.19.0",
    "vite": "^7.1.7",
    "vitest": "^3.2.4"
  }
}
